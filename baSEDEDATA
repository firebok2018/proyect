
drop database taller
create database Taller

use Taller

create schema RRHH
create schema produc
create schema 

create table cliente(
Idcli char(4)  not null ,
nombreCli varchar(35) not null,
apellCli varchar(35) not null,
telefonoCli char(9) null,
dniCli char(8) not null,
primary key (Idcli)
)

create table producto(
idproduc char(4) not null primary key,
nomproduc varchar(50) not null,
catproduc varchar (40) not null,
cantproduc int not null,
precio  double not null
)
